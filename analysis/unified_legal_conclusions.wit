action declarar_nulidad_judicial = "judicially declare title nullity", prescripcion_legal;
action carecer_efecto_juridico = "lack any legal effect due to nullification", cumplimiento_contractual;
action ser_inexistente_acto = "be a legally inexistent act", cumplimiento_contractual;
action generar_certidumbre_propiedad = "generate certainty in property ownership", proteccion_legal;

// Additional assets for judicial nullification effects
asset nulidad_judicial_titulo = tribunal, declarar_nulidad_judicial, demandante;
asset ausencia_efecto_juridico = demandante, carecer_efecto_juridico, tribunal;
asset acto_inexistente = demandante, ser_inexistente_acto, tribunal;
asset certidumbre_propiedad = tribunal, generar_certidumbre_propiedad, titular_registral;

// Faith exclusivity: Adverse possession is either good faith XOR bad faith
clause tesis2030500_exclusividad_fe = claim(demanda_prescripcion) IMPLIES (oblig(ejercicio_buena_fe) XOR oblig(ejercicio_mala_fe));

// Thesis-specific principle: Judicially nullified title cannot establish generating cause for adverse possession
clause tesis2030500_principio_fundamental = oblig(nulidad_judicial_titulo) IMPLIES not(oblig(causa_generadora_probada));

// Nullification effect: Judicially nullified acts lack legal effect
clause tesis2030500_efecto_nulidad = oblig(nulidad_judicial_titulo) IMPLIES oblig(ausencia_efecto_juridico);

// Inexistent act consequence: Nullified acts are treated as legally inexistent
clause tesis2030500_acto_inexistente = oblig(ausencia_efecto_juridico) IMPLIES oblig(acto_inexistente);

// Ownership character impossibility: Nullified title cannot establish ownership possession
clause tesis2030500_imposibilidad_propietario = oblig(acto_inexistente) IMPLIES not(oblig(posesion_como_propietario));

// Good faith impossibility: Nullified title prevents good faith establishment
clause tesis2030500_imposibilidad_buena_fe = oblig(nulidad_judicial_titulo) AND claim(demanda_prescripcion) IMPLIES oblig(ejercicio_mala_fe);

// Public order purpose: Adverse possession serves property certainty
clause tesis2030500_orden_publico = claim(demanda_prescripcion) IMPLIES oblig(certidumbre_propiedad);

// Thesis validation
asset tesis2030500_valida = global();
